@model IEnumerable<MVCcoreWebAppHTD1.Models.studentdatamodel>
@{
    ViewData["Title"] = "displaydata";
    Layout = "~/Views/Shared/_userLayout.cshtml";
}
<html>
<head>
   

    <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css" rel="stylesheet">

    <script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="form-outline">
        <i class="fa fa-search"></i>
        <input type="search" id="myInput" class="form" placeholder="Search Here...">
        <span class="right-pan color: red;"><i></i></span>
    </div>

  <table class="table table-success">
   <thead class="table-dark">
 

    <tr>
        <th>RefID</th>
        <th>Name</th>
        <th>EmailID</th>
        <th>Password</th>
        <th>Mobile</th>

 

        <th>Gender</th>
        <th>DOB</th>
        <th>Fee</th>
        <th>Branch</th>
        <th>Status</th>
        <th>role </th>
      
       
        <th>Action</th>
    </tr>
    </thead>
        <tbody id="myTable">

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.RefID</td>

 

            <td>@item.Name</td>

 

            <td>@item.EmailId</td>

 

            <td>@item.Password</td>

 

            <td>@item.Mobile</td>

 

            <td>@item.Gender</td>

 

            <td>@item.DOB</td>

 

            <td>@item.Fee</td>

 
            <td>@item.Branch</td>

 
            <td>@item.status</td>



              <td>@item.role</td>


          

               <td>
                  @* @Html.ActionLink("Update","Edit","student", new{RefID=item.RefID},null)*@
                    <a asp-action="Edit" asp-route-RefID="@item.RefID"><i class="text-sucess"><i class="fa fa-edit fa-lg btn-lg ;"></i></a>
                   @*@Html.ActionLink("Delete","Delete","student", new{RefID=item.RefID}, new { onclick = "return confirm('Are you sure wants to delete?');" })*@
                    <a asp-action="Delete" asp-route-RefID="@item.RefID">
                        <i class=" text-danger ml-1" onclick="return confirm('Are you sure to delete this record..?')"><i class="fa fa-trash fa-lg btn-lg"></i></i>
                    </a>
               
               </td>

        </tr>
    }

 


</table>


<script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
<script src="https://unpkg.com/popper.js@1.12.6/dist/umd/popper.js" integrity="sha384-fA23ZRQ3G/J53mElWqVJEGJzU0sTs+SvzG8fXVWP+kJQ1lwFAOkcUOysnlKJC33U" crossorigin="anonymous"></script>
<script src="https://unpkg.com/bootstrap-material-design@4.1.1/dist/js/bootstrap-material-design.js" integrity="sha384-CauSuKpEqAFajSpkdjv3z9t8E7RlpJ1UP0lKM/+NdtSarroVKu069AlsRPKkFBz9" crossorigin="anonymous"></script>
@* //search script*@
<script>
    $(document).ready(function () {
        $("#myInput").on("keyup", function () {
            var value = $(this).val().toLowerCase();
            $("#myTable tr").filter(function () {
                $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
        });
    });
</script>
</body>