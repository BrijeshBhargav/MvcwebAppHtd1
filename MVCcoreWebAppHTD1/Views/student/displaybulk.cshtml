@using MVCcoreWebAppHTD1.Models
@model bulkmodel
@{
    ViewData["Title"] = "AddData";
    Layout = "~/Views/Shared/_userLayout.cshtml";
}

<h1>AddData</h1>

<html lang="en">
<head>
  <title>Bootstrap Example</title>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"></script>
</head>
<body>

<div class="container mt-3">
  <h2>Jq insert</h2>
 using( @Html.BeginForm())
  {
@*  <form action="/action_page.php">*@
    <div class="col-md-6 mb-6">
      <label for="email">EmployeId:</label>
         @Html.TextBoxFor(model => model.EmployeeID, new { @class = "form-control", placeholder="Enter Employeeid" })
     
    </div>
   <div class="col-md-6 mb-6">
      <label for="pwd">Name:</label>
     @Html.TextBoxFor(model => model.Name, new { @class = "form-control", placeholder="Enter Name" })
    </div>
   <div class="col-md-6 mb-6">
      <label for="pwd">Role:</label>
     @Html.TextBoxFor(model => model.Role, new { @class = "form-control", placeholder="Enter Role" })
    </div>
   <div class="col-md-6 mb-6">
      <label for="pwd">Age:</label>
     @Html.TextBoxFor(model => model.Age, new { @class = "form-control", placeholder="Enter Age" })
    </div>
  <div class="col-md-6 mb-6">
      <label for="pwd">Salary:</label>
     @Html.TextBoxFor(model => model.Salary, new { @class = "form-control", placeholder="Enter Salary" })
    </div>
    <div class="form-check mb-3">
      <button type="submit" class="btn btn-primary">Submit</button>
    </div>

  
  </body>
  </html>

